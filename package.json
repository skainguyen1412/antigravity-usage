{
  "name": "antigravity-usage",
  "version": "0.2.8",
  "description": "CLI tool to check Antigravity model quota via Google Cloud Code API",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "antigravity-usage": "dist/index.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsx src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "antigravity",
    "quota",
    "cli",
    "google-cloud-code"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@types/inquirer": "^9.0.9",
    "cli-table3": "^0.6.5",
    "commander": "^12.1.0",
    "inquirer": "^9.3.8",
    "open": "^10.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^4.0.18"
  }
}
